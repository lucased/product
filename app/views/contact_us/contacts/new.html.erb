<div id="wrapper"><!-- wrapper start -->

	<div id="top_wrapper"><!-- top wrapper start -->
		<div id="top_wrapper_line_t"></div><!-- top line of the top wrapper -->

		<div id="header" class="broof_grid"><!-- header start -->

			<a href="<% root_path %>" id="logo"><img src="../images/logo.png" alt="logo" /></a><!-- Your logo here -->

			<%= render "pages/nav"%>

			<div class="clear"></div><!-- clear navigation and logo -->					

		</div><!-- header end -->

	</div><!-- top wrapper end -->
	<div id="top_wrapper_line_g"></div><!-- bottom line of the top wrapper -->

	<div class="broof_grid"><!-- content start -->

		<div class="two_third has_sidebar_right">
			
			<div id="contact_form">
			<h2><%= t('.contact_us') %></h2>
			<p>Please fill in the form below and we will answer your question as soon as possible.</p>
			<%= semantic_form_for @contact, :url => contacts_path do |f| %>
			<%= f.input :name, :label => t('.name') if ContactUs.require_name %>
			<%= f.input :email, :label => t('.email') %>
			<%= f.input :subject, :label => t('.subject') if ContactUs.require_subject %>
			<%= f.input :message, :as => :text, :label => t('.message') %>
			<%= f.action :submit, :label => t('.submit'), :button_html => { :alt => t('.submit'), :id => 'contact_us_contact_submit', :title => t('.submit') } %>
			<% end %>
			</div><!-- #contact_form -->
			
			

			<div class="devider_side"></div>

		</div>

		<div class="one_third sidebar_right_repeat last">
			<div class="sidebar_right_top">

